<% @title = "#{@user_id}さんの投稿記事一覧" %>
<h1><%= @title %></h1>
<div>
    <table>
        <tr>
            <th>作成日</th>
            <th>タイトル</th>
        </tr>
        <% @posts.each do |post| %>
            <tr>
                <td><%= post.created_at.strftime('%Y/%m/%d') %></td>
                <td><%= link_to post.title, post_path(post) %></td>
            </tr>
        <% end %>
    </table>
</div>